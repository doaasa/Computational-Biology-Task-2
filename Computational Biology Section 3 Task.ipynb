   "source": [
    "from pyopenms import *\n",
    "\n",
    "seq = AASequence.fromString(\"AVKA\")\n",
    "\n",
    "weight=seq.getMonoWeight()\n",
    "\n",
    "weightForEachAA=0\n",
    "\n",
    "for AA in seq:\n",
    "    weightForEachAA= weightForEachAA+ AA.getMonoWeight()\n",
    "    \n",
    "if(weight == weightForEachAA):\n",
    "    print(\"Equal Weight\")\n",
    "\n",
    "else:\n",
    "    print(\"Not Equal Weight\")\n",
    "    "
   ]
